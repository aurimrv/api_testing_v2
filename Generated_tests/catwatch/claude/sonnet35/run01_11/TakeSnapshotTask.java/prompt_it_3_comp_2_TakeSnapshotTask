    @Test
    public void testCall() throws Exception {
        GitHub gitHub = Mockito.mock(GitHub.class);
        String organizationName = "testOrg";
        Scorer scorer = Mockito.mock(Scorer.class);
        Date snapshotDate = new Date();

        OrganizationWrapper orgWrapper = Mockito.mock(OrganizationWrapper.class);
        Mockito.when(gitHub.getOrganization(organizationName)).thenReturn(null);
        Mockito.when(orgWrapper.getId()).thenReturn(1L);

        TakeSnapshotTask task = new TakeSnapshotTask(gitHub, organizationName, scorer, snapshotDate);
        Snapshot snapshot = task.call();

        assertNotNull(snapshot);
        // Add more assertions based on the expected behavior
    }

    @Test
    public void testCollectStatistics() throws IOException {
        GitHub gitHub = Mockito.mock(GitHub.class);
        String organizationName = "testOrg";
        Scorer scorer = Mockito.mock(Scorer.class);
        Date snapshotDate = new Date();

        OrganizationWrapper orgWrapper = Mockito.mock(OrganizationWrapper.class);
        Mockito.when(orgWrapper.getId()).thenReturn(1L);
        Mockito.when(orgWrapper.getLogin()).thenReturn(organizationName);

        TakeSnapshotTask task = new TakeSnapshotTask(gitHub, organizationName, scorer, snapshotDate);
        Statistics statistics = task.collectStatistics(orgWrapper);

        assertNotNull(statistics);
        assertEquals(1L, statistics.getId());
        assertEquals(organizationName, statistics.getOrganizationName());
        assertEquals(snapshotDate, statistics.getSnapshotDate());
        // Add more assertions for other statistics fields
    }

    @Test
    public void testCollectProjects() throws IOException, URISyntaxException {
