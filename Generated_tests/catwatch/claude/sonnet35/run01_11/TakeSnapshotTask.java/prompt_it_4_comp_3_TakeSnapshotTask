package org.zalando.catwatch.backend;

import org.junit.AfterClass;
import org.junit.BeforeClass;
import org.junit.Before;
import org.junit.Test;
import static org.junit.Assert.*;
import java.util.Map;
import java.util.List;
import java.util.Collection;
import org.zalando.catwatch.backend.github.TakeSnapshotTask;
import org.zalando.catwatch.backend.model.Snapshot;
import org.zalando.catwatch.backend.model.Statistics;
import org.zalando.catwatch.backend.model.Project;
import org.zalando.catwatch.backend.model.Contributor;
import org.zalando.catwatch.backend.model.Language;
import org.zalando.catwatch.backend.model.util.Scorer;
import org.kohsuke.github.GitHub;
import org.kohsuke.github.GHOrganization;
import org.kohsuke.github.GHRepository;
import org.mockito.Mockito;

import java.util.Date;
import java.util.ArrayList;
import java.util.HashMap;
import java.io.IOException;

public class sonnet35_run01_TakeSnapshotTaskTest {

    private TakeSnapshotTask takeSnapshotTask;
    private GitHub mockGitHub;
    private GHOrganization mockOrganization;
    private Scorer mockScorer;

    @Before
    public void initTest() throws Exception {
        mockGitHub = Mockito.mock(GitHub.class);
        mockOrganization = Mockito.mock(GHOrganization.class);
        mockScorer = Mockito.mock(Scorer.class);

        Mockito.when(mockGitHub.getOrganization(Mockito.anyString())).thenReturn(mockOrganization);

        takeSnapshotTask = new TakeSnapshotTask(mockGitHub, "testOrg", mockScorer, new Date());
    }

    @Test
    public void testCall() throws Exception {
        Mockito.when(mockOrganization.listRepositories()).thenReturn(new ArrayList<GHRepository>());
        Mockito.when(mockOrganization.listMembers()).thenReturn(new ArrayList<>());
        Mockito.when(mockOrganization.listTeams()).thenReturn(new ArrayList<>());

        Snapshot snapshot = takeSnapshotTask.call();

        assertNotNull(snapshot);
        assertNotNull(snapshot.getStatistics());
        assertNotNull(snapshot.getProjects());
        assertNotNull(snapshot.getContributors());
        assertNotNull(snapshot.getLanguages());
    }

    @Test
    public void testCollectStatistics() throws Exception {
        Mockito.when(mockOrganization.listRepositories()).thenReturn(new ArrayList<GHRepository>());
        Mockito.when(mockOrganization.listMembers()).thenReturn(new ArrayList<>());
        Mockito.when(mockOrganization.listTeams()).thenReturn(new ArrayList<>());

        Statistics statistics = takeSnapshotTask.collectStatistics(mockOrganization);

        assertNotNull(statistics);
        assertEquals(0, statistics.getPublicProjectCount());
        assertEquals(0, statistics.getMembersCount());
        assertEquals(0, statistics.getTeamsCount());
    }

    @Test
    public void testCollectProjects() throws Exception {
        GHRepository mockRepo = Mockito.mock(GHRepository.class);
        Mockito.when(mockOrganization.listRepositories()).thenReturn(new ArrayList<GHRepository>() {{ add(mockRepo); }});
        Mockito.when(mockRepo.getName()).thenReturn("testRepo");
        Mockito.when(mockRepo.getDescription()).thenReturn("Test Description");
        Mockito.when(mockRepo.getStargazersCount()).thenReturn(10);
        Mockito.when(mockRepo.getForksCount()).thenReturn(5);
        Mockito.when(mockRepo.getPushedAt()).thenReturn(new Date());
        Mockito.when(mockRepo.getLanguage()).thenReturn("Java");
        Mockito.when(mockRepo.listLanguages()).thenReturn(new HashMap<>());

        Collection<Project> projects = takeSnapshotTask.collectProjects(mockOrganization);

        assertNotNull(projects);
        assertEquals(1, projects.size());
        Project project = projects.iterator().next();
        assertEquals("testRepo", project.getName());
        assertEquals("Test Description", project.getDescription());
        assertEquals(10, project.getStarsCount());
        assertEquals(5, project.getForksCount());
        assertEquals("Java", project.getPrimaryLanguage());
    }

    @Test
    public void testCollectContributors() throws Exception {
        GHRepository mockRepo = Mockito.mock(GHRepository.class);
        GHRepository.Contributor mockContributor = Mockito.mock(GHRepository.Contributor.class);
        Mockito.when(mockOrganization.listRepositories()).thenReturn(new ArrayList<GHRepository>() {{ add(mockRepo); }});
        Mockito.when(mockRepo.listContributors()).thenReturn(new ArrayList<GHRepository.Contributor>() {{ add(mockContributor); }});
        Mockito.when(mockContributor.getId()).thenReturn(1);
        Mockito.when(mockContributor.getName()).thenReturn("testContributor");
        Mockito.when(mockContributor.getContributions()).thenReturn(10);

        Collection<Contributor> contributors = takeSnapshotTask.collectContributors(mockOrganization);

        assertNotNull(contributors);
        assertEquals(1, contributors.size());
        Contributor contributor = contributors.iterator().next();
        assertEquals("testContributor", contributor.getName());
        assertEquals(10, contributor.getOrganizationalCommitsCount());
    }

    @Test
    public void testCollectLanguages() throws Exception {
        GHRepository mockRepo = Mockito.mock(GHRepository.class);
        Map<String, Long> languages = new HashMap<>();
        languages.put("Java", 1000L);
        languages.put("Python", 500L);
        Mockito.when(mockOrganization.listRepositories()).thenReturn(new ArrayList<GHRepository>() {{ add(mockRepo); }});
        Mockito.when(mockRepo.listLanguages()).thenReturn(languages);

        Collection<Language> collectedLanguages = takeSnapshotTask.collectLanguages(mockOrganization);

        assertNotNull(collectedLanguages);
        assertEquals(2, collectedLanguages.size());
        for (Language lang : collectedLanguages) {
            if (lang.getName().equals("Java")) {
                assertEquals(66, lang.getPercentage());
            } else if (lang.getName().equals("Python")) {
                assertEquals(33, lang.getPercentage());
            } else {
                fail("Unexpected language: " + lang.getName());
            }
        }
    }

    @Test
    public void testGetProjectMaintainers() throws Exception {
        GHRepository mockRepo = Mockito.mock(GHRepository.class);
        Mockito.when(mockRepo.getFileContent("MAINTAINERS")).thenReturn("maintainer1\nmaintainer2");

        List<String> maintainers = takeSnapshotTask.getProjectMaintainers(mockRepo);

        assertNotNull(maintainers);
        assertEquals(2, maintainers.size());
        assertTrue(maintainers.contains("maintainer1"));
        assertTrue(maintainers.contains("maintainer2"));
    }

    @Test
    public void testReadCatwatchYaml() throws Exception {
        GHRepository mockRepo = Mockito.mock(GHRepository.class);
        Project project = new Project();
        Mockito.when(mockRepo.getFileContent(".catwatch.yaml")).thenReturn("title: Test Title\nimage: test.jpg");

        takeSnapshotTask.readCatwatchYaml(mockRepo, project);

        assertEquals("Test Title", project.getTitle());
        assertEquals("test.jpg", project.getImage());
    }

    @Test(expected = IOException.class)
    public void testReadCatwatchYamlWithIOException() throws Exception {
        GHRepository mockRepo = Mockito.mock(GHRepository.class);
        Project project = new Project();
        Mockito.when(mockRepo.getFileContent(".catwatch.yaml")).thenThrow(new IOException());

        takeSnapshotTask.readCatwatchYaml(mockRepo, project);
    }
}

