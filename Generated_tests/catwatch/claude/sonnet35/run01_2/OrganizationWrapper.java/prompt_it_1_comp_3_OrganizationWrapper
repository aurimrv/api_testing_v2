package org.zalando.catwatch.backend;

import org.junit.Test;
import static org.junit.Assert.*;
import org.mockito.Mockito;
import static org.mockito.Mockito.*;
import org.kohsuke.github.*;
import java.io.IOException;
import java.util.Collections;
import java.util.List;
import org.zalando.catwatch.backend.github.OrganizationWrapper;
import org.zalando.catwatch.backend.github.RepositoryWrapper;

public class sonnet35_run01_OrganizationWrapperTest {

    @Test
    public void testConstructor() throws Exception {
        GHOrganization mockOrg = Mockito.mock(GHOrganization.class);
        when(mockOrg.listRepositories()).thenReturn(Collections.emptyList());
        
        OrganizationWrapper wrapper = new OrganizationWrapper(mockOrg);
        
        assertNotNull(wrapper);
    }

    @Test
    public void testListTeams() throws Exception {
        GHOrganization mockOrg = Mockito.mock(GHOrganization.class);
        GHTeam mockTeam = Mockito.mock(GHTeam.class);
        when(mockOrg.listTeams()).thenReturn(Collections.singletonList(mockTeam));
        
        OrganizationWrapper wrapper = new OrganizationWrapper(mockOrg);
        
        List<GHTeam> teams = wrapper.listTeams();
        assertNotNull(teams);
        assertEquals(1, teams.size());
    }

    @Test
    public void testListMembers() throws Exception {
        GHOrganization mockOrg = Mockito.mock(GHOrganization.class);
        GHUser mockUser = Mockito.mock(GHUser.class);
        when(mockOrg.listMembers()).thenReturn(Collections.singletonList(mockUser));
        
        OrganizationWrapper wrapper = new OrganizationWrapper(mockOrg);
        
        List<GHUser> members = wrapper.listMembers();
        assertNotNull(members);
        assertEquals(1, members.size());
    }

    @Test
    public void testListRepositories() throws Exception {
        GHOrganization mockOrg = Mockito.mock(GHOrganization.class);
        GHRepository mockRepo = Mockito.mock(GHRepository.class);
        when(mockRepo.isPrivate()).thenReturn(false);
        when(mockRepo.isFork()).thenReturn(false);
        when(mockOrg.listRepositories()).thenReturn(Collections.singletonList(mockRepo));
        
        OrganizationWrapper wrapper = new OrganizationWrapper(mockOrg);
        
        List<RepositoryWrapper> repos = wrapper.listRepositories();
        assertNotNull(repos);
        assertEquals(1, repos.size());
    }

    @Test
    public void testGetId() throws Exception {
        GHOrganization mockOrg = Mockito.mock(GHOrganization.class);
        when(mockOrg.getId()).thenReturn(123);
        
        OrganizationWrapper wrapper = new OrganizationWrapper(mockOrg);
        
        assertEquals(123, wrapper.getId());
    }

    @Test
    public void testGetPublicRepoCount() throws Exception {
        GHOrganization mockOrg = Mockito.mock(GHOrganization.class);
        when(mockOrg.getPublicRepoCount()).thenReturn(5);
        
        OrganizationWrapper wrapper = new OrganizationWrapper(mockOrg);
        
        assertEquals(5, wrapper.getPublicRepoCount());
    }

    @Test
    public void testGetPublicRepoCountWithException() throws Exception {
        GHOrganization mockOrg = Mockito.mock(GHOrganization.class);
        when(mockOrg.getPublicRepoCount()).thenThrow(new IOException());
        
        OrganizationWrapper wrapper = new OrganizationWrapper(mockOrg);
        
        assertEquals(0, wrapper.getPublicRepoCount());
    }

    @Test
    public void testGetLogin() throws Exception {
        GHOrganization mockOrg = Mockito.mock(GHOrganization.class);
        when(mockOrg.getLogin()).thenReturn("testOrg");
        
        OrganizationWrapper wrapper = new OrganizationWrapper(mockOrg);
        
        assertEquals("testOrg", wrapper.getLogin());
    }

    @Test
    public void testContributorIsMember() throws Exception {
        GHOrganization mockOrg = Mockito.mock(GHOrganization.class);
        GHUser mockUser = Mockito.mock(GHUser.class);
        GHRepository.Contributor mockContributor = Mockito.mock(GHRepository.Contributor.class);
        when(mockOrg.listMembers()).thenReturn(Collections.singletonList(mockUser));
        
        OrganizationWrapper wrapper = new OrganizationWrapper(mockOrg);
        
        assertFalse(wrapper.contributorIsMember(mockContributor));
    }
}

