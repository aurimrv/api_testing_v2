public class sonnet35_run01_StatisticsBuilderTest {

    private static final SutHandler controller = new em.embedded.org.zalando.EmbeddedEvoMasterController();
    private static String baseUrlOfSut;
    private StatisticsRepository mockRepository;
    private StatisticsBuilder statisticsBuilder;

    @BeforeClass
    public static void initClass() {
        controller.setupForGeneratedTest();
        baseUrlOfSut = controller.startSut();
        controller.registerOrExecuteInitSqlCommandsIfNeeded();
        assertNotNull(baseUrlOfSut);
        RestAssured.enableLoggingOfRequestAndResponseIfValidationFails();
        RestAssured.useRelaxedHTTPSValidation();
        RestAssured.urlEncodingEnabled = false;
        RestAssured.config = RestAssured.config()
            .jsonConfig(JsonConfig.jsonConfig().numberReturnType(JsonPathConfig.NumberReturnType.DOUBLE))
            .redirect(redirectConfig().followRedirects(false));
    }

    @AfterClass
    public static void tearDown() {
        controller.stopSut();
    }

    @Before
    public void initTest() {
        controller.resetDatabase(Arrays.asList("CONTRIBUTOR"));
        controller.resetStateOfSUT();
        mockRepository = Mockito.mock(StatisticsRepository.class);
        statisticsBuilder = new StatisticsBuilder(mockRepository);
    }

    @Test
    public void testConstructor() {
        assertNotNull(statisticsBuilder);
        Statistics stats = statisticsBuilder.create();
        assertNotNull(stats);
        assertNotNull(stats.getId());
        assertNotNull(stats.getSnapshotDate());
    }

    @Test
    public void testOrganizationName() {
        String orgName = "TestOrg";
        statisticsBuilder.organizationName(orgName);
        Statistics stats = statisticsBuilder.create();
        assertEquals(orgName, stats.getOrganizationName());
    }

    @Test
    public void testPublicProjectCount() {
        int count = 10;
        statisticsBuilder.publicProjectCount(count);
        Statistics stats = statisticsBuilder.create();
        assertEquals((long)count, stats.getPublicProjectCount());
    }

    @Test
    public void testAllStarsCount() {
        int count = 100;
        statisticsBuilder.allStarsCount(count);
        Statistics stats = statisticsBuilder.create();
        assertEquals((long)count, stats.getAllStarsCount());
    }

    @Test
    public void testAllForksCount() {
        int count = 50;
        statisticsBuilder.allForksCount(count);
        Statistics stats = statisticsBuilder.create();
        assertEquals((long)count, stats.getAllForksCount());
    }

    @Test
    public void testProgramLanguagesCount() {
        int count = 5;
        statisticsBuilder.programLanguagesCount(count);
        Statistics stats = statisticsBuilder.create();
        assertEquals((long)count, stats.getProgramLanguagesCount());
    }

    @Test
    public void testAllContributersCount() {
        int count = 20;
        statisticsBuilder.allContributersCount(count);
        Statistics stats = statisticsBuilder.create();
        assertEquals((long)count, stats.getAllContributorsCount());
    }

    @Test
    public void testExternalContributorsCount() {
        int count = 15;
        statisticsBuilder.externalContributorsCount(count);
        Statistics stats = statisticsBuilder.create();
        assertEquals((long)count, stats.getExternalContributorsCount());
    }

    @Test
    public void testAllSize() {
        int size = 1000;
        statisticsBuilder.allSize(size);
        Statistics stats = statisticsBuilder.create();
        assertEquals((long)size, stats.getAllSizeCount());
    }

    @Test
    public void testMembersCount() {
        int count = 30;
        statisticsBuilder.membersCount(count);
        Statistics stats = statisticsBuilder.create();
        assertEquals((long)count, stats.getMembersCount());
    }

    @Test
    public void testPrivateProjectCount() {
        int count = 5;
        statisticsBuilder.privateProjectCount(count);
        Statistics stats = statisticsBuilder.create();
        assertEquals((long)count, stats.getPrivateProjectCount());
    }

    @Test
    public void testTagsCount() {
        int count = 25;
        statisticsBuilder.tagsCount(count);
        Statistics stats = statisticsBuilder.create();
        assertEquals((long)count, stats.getTagsCount());
    }

    @Test
    public void testTeamsCount() {
        int count = 8;
        statisticsBuilder.teamsCount(count);
        Statistics stats = statisticsBuilder.create();
        assertEquals((long)count, stats.getTeamsCount());
    }

    @Test
    public void testSnapshotDate() {
        Date date = new Date();
        statisticsBuilder.snapshotDate(date);
        Statistics stats = statisticsBuilder.create();
        assertEquals(date, stats.getSnapshotDate());
    }

    @Test
    public void testDays() {
        int days = 7;
        statisticsBuilder.days(days);
        Statistics stats = statisticsBuilder.create();
        long expectedTime = new Date().getTime() - (days * 24 * 60 * 60 * 1000L);
        assertTrue(Math.abs(stats.getSnapshotDate().getTime() - expectedTime) < 1000); // Allow 1 second difference
    }

    @Test
    public void testCreate() {
        Statistics stats = statisticsBuilder
            .organizationName("TestOrg")
            .publicProjectCount(10)
            .privateProjectCount(5)
            .membersCount(20)
            .teamsCount(3)
            .allContributersCount(30)
            .externalContributorsCount(15)
            .allStarsCount(100)
            .allForksCount(50)
            .allSize(1000)
            .programLanguagesCount(5)
            .tagsCount(25)
            .create();

        assertNotNull(stats);
        assertEquals("TestOrg", stats.getOrganizationName());
        assertEquals(10L, stats.getPublicProjectCount());
        assertEquals(5L, stats.getPrivateProjectCount());
        assertEquals(20L, stats.getMembersCount());
        assertEquals(3L, stats.getTeamsCount());
        assertEquals(30L, stats.getAllContributorsCount());
        assertEquals(15L, stats.getExternalContributorsCount());
        assertEquals(100L, stats.getAllStarsCount());
        assertEquals(50L, stats.getAllForksCount());
        assertEquals(1000L, stats.getAllSizeCount());
        assertEquals(5L, stats.getProgramLanguagesCount());
        assertEquals(25L, stats.getTagsCount());
    }

    @Test
    public void testSave() {
        Statistics stats = statisticsBuilder.create();
        when(mockRepository.save(any(Statistics.class))).thenReturn(stats);

        Statistics savedStats = statisticsBuilder.save();
        assertNotNull(savedStats);
        verify(mockRepository).save(any(Statistics.class));
    }
}

