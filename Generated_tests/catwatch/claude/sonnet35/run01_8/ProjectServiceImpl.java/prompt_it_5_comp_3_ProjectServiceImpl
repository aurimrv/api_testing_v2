package org.zalando.catwatch.backend;

import org.junit.Before;
import org.junit.Test;
import static org.junit.Assert.*;
import static org.mockito.Mockito.*;

import org.zalando.catwatch.backend.model.Project;
import org.zalando.catwatch.backend.repo.ProjectRepository;
import org.zalando.catwatch.backend.service.ProjectServiceImpl;
import org.springframework.core.env.Environment;

import java.util.*;

public class sonnet35_run01_ProjectServiceImplTest {

    private ProjectServiceImpl projectService;
    private ProjectRepository mockProjectRepository;
    private Environment mockEnvironment;

    @Before
    public void initTest() {
        mockProjectRepository = mock(ProjectRepository.class);
        mockEnvironment = mock(Environment.class);
        projectService = new ProjectServiceImpl(mockProjectRepository, mockEnvironment);
    }

    @Test
    public void testFindProjects() {
        String organizations = "org1,org2";
        Optional<Integer> limit = Optional.of(10);
        Optional<Integer> offset = Optional.of(0);
        Optional<Date> startDate = Optional.of(new Date());
        Optional<Date> endDate = Optional.of(new Date());
        Optional<String> sortBy = Optional.of("stars");
        Optional<String> query = Optional.of("test");
        Optional<String> language = Optional.of("Java");

        List<Project> mockProjects = Arrays.asList(new Project(), new Project());
        when(mockProjectRepository.findProjects(anyString(), any(Date.class), anyString(), anyString())).thenReturn(mockProjects);
        when(mockEnvironment.getProperty(anyString())).thenReturn("org1,org2");

        Iterable<Project> result = projectService.findProjects(organizations, limit, offset, startDate, endDate, sortBy, query, language);

        assertNotNull(result);
        assertTrue(result.iterator().hasNext());
        verify(mockProjectRepository, times(4)).findProjects(anyString(), any(Date.class), anyString(), anyString());
    }

    @Test
    public void testGetMergedProjectList() {
        List<Project> startProjects = Arrays.asList(
            createProject(1L, 10, 100, 1000, 50, 500),
            createProject(2L, 20, 200, 2000, 100, 1000)
        );
        List<Project> endProjects = Arrays.asList(
            createProject(1L, 15, 150, 1500, 75, 750),
            createProject(2L, 25, 250, 2500, 125, 1250),
            createProject(3L, 30, 300, 3000, 150, 1500)
        );

        List<Project> result = invokeGetMergedProjectList(projectService, startProjects, endProjects);

        assertEquals(3, result.size());
        assertEquals(5, result.get(0).getStarsCount());
        assertEquals(50, result.get(0).getCommitsCount());
        assertEquals(500, result.get(0).getForksCount());
        assertEquals(25, result.get(0).getContributorsCount());
        assertEquals(250, result.get(0).getScore());
        assertEquals(30, result.get(2).getStarsCount());
    }

    @Test
    public void testCreateMergedProject() {
        Project startProject = createProject(1L, 10, 100, 1000, 50, 500);
        Project endProject = createProject(1L, 15, 150, 1500, 75, 750);

        Project result = invokeCreateMergedProject(projectService, startProject, endProject);

        assertEquals(5, result.getStarsCount());
        assertEquals(50, result.getCommitsCount());
        assertEquals(500, result.getForksCount());
        assertEquals(25, result.getContributorsCount());
        assertEquals(250, result.getScore());
    }

    @Test
    public void testConvertProjectsToMap() {
        List<Project> projects = Arrays.asList(
            createProject(1L, 10, 100, 1000, 50, 500),
            createProject(2L, 20, 200, 2000, 100, 1000)
        );

        Map<Long, Project> result = invokeConvertProjectsToMap(projectService, projects);

        assertEquals(2, result.size());
        assertTrue(result.containsKey(1L));
        assertTrue(result.containsKey(2L));
        assertEquals(10, result.get(1L).getStarsCount());
        assertEquals(20, result.get(2L).getStarsCount());
    }

    @Test
    public void testGetOrganizationConfig() {
        when(mockEnvironment.containsProperty("organization.list")).thenReturn(true);
        when(mockEnvironment.getProperty("organization.list")).thenReturn("org1,org2,org3");

        String result = invokeGetOrganizationConfig(projectService);

        assertEquals("org1,org2,org3", result);
    }

    @Test
    public void testGetOrganizationConfigEmpty() {
        when(mockEnvironment.containsProperty("organization.list")).thenReturn(false);

        String result = invokeGetOrganizationConfig(projectService);

        assertEquals("", result);
    }

    @Test
    public void testIsSortOrderAscending() {
        assertTrue(invokeIsSortOrderAscending(projectService, Optional.of("name")));
        assertFalse(invokeIsSortOrderAscending(projectService, Optional.of("-name")));
        assertFalse(invokeIsSortOrderAscending(projectService, Optional.empty()));
    }

    private Project createProject(Long id, int stars, int commits, int forks, int contributors, int score) {
        Project project = new Project();
        project.setGitHubProjectId(id);
        project.setStarsCount(stars);
        project.setCommitsCount(commits);
        project.setForksCount(forks);
        project.setContributorsCount(contributors);
        project.setScore(score);
        return project;
    }

    // Helper methods to invoke private methods using reflection
    private List<Project> invokeGetMergedProjectList(ProjectServiceImpl service, List<Project> startProjects, List<Project> endProjects) {
        try {
            java.lang.reflect.Method method = ProjectServiceImpl.class.getDeclaredMethod("getMergedProjectList", List.class, List.class);
            method.setAccessible(true);
            return (List<Project>) method.invoke(service, startProjects, endProjects);
        } catch (Exception e) {
            throw new RuntimeException(e);
        }
    }

    private Project invokeCreateMergedProject(ProjectServiceImpl service, Project startProject, Project endProject) {
        try {
            java.lang.reflect.Method method = ProjectServiceImpl.class.getDeclaredMethod("createMergedProject", Project.class, Project.class);
            method.setAccessible(true);
            return (Project) method.invoke(service, startProject, endProject);
        } catch (Exception e) {
            throw new RuntimeException(e);
        }
    }

    private Map<Long, Project> invokeConvertProjectsToMap(ProjectServiceImpl service, List<Project> projects) {
        try {
            java.lang.reflect.Method method = ProjectServiceImpl.class.getDeclaredMethod("convertProjectsToMap", List.class);
            method.setAccessible(true);
            return (Map<Long, Project>) method.invoke(service, projects);
        } catch (Exception e) {
            throw new RuntimeException(e);
        }
    }

    private String invokeGetOrganizationConfig(ProjectServiceImpl service) {
        try {
            java.lang.reflect.Method method = ProjectServiceImpl.class.getDeclaredMethod("getOrganizationConfig");
            method.setAccessible(true);
            return (String) method.invoke(service);
        } catch (Exception e) {
            throw new RuntimeException(e);
        }
    }

    private boolean invokeIsSortOrderAscending(ProjectServiceImpl service, Optional<String> sortBy) {
        try {
            java.lang.reflect.Method method = ProjectServiceImpl.class.getDeclaredMethod("isSortOrderAscending", Optional.class);
            method.setAccessible(true);
            return (boolean) method.invoke(service, sortBy);
        } catch (Exception e) {
            throw new RuntimeException(e);
        }
    }
}

