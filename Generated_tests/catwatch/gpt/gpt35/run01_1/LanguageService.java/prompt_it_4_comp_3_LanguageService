import org.evomaster.client.java.controller.expect.ExpectationHandler;
import io.restassured.path.json.JsonPath;
import java.util.Arrays;
import java.util.ArrayList;
import java.util.Comparator;
import java.util.Optional;
import org.zalando.catwatch.backend.Language;
import org.zalando.catwatch.backend.LanguageService;
import org.zalando.catwatch.backend.ProjectRepository;

public class gpt35_run01_LanguageServiceTest {

    private static final SutHandler controller = new em.embedded.org.zalando.EmbeddedEvoMasterController();
    private static String baseUrlOfSut;

    @BeforeClass
    public static void initClass() {
        controller.setupForGeneratedTest();
        baseUrlOfSut = controller.startSut();
        controller.registerOrExecuteInitSqlCommandsIfNeeded();
        assertNotNull(baseUrlOfSut);
        RestAssured.enableLoggingOfRequestAndResponseIfValidationFails();
        RestAssured.useRelaxedHTTPSValidation();
        RestAssured.urlEncodingEnabled = false;
        RestAssured.config = RestAssured.config()
            .jsonConfig(JsonConfig.jsonConfig().numberReturnType(JsonPathConfig.NumberReturnType.DOUBLE))
            .redirect(redirectConfig().followRedirects(false));
    }

    @AfterClass
    public static void tearDown() {
        controller.stopSut();
    }

    @Before
    public void initTest() {
        controller.resetDatabase(Arrays.asList("CONTRIBUTOR"));
        controller.resetStateOfSUT();
    }

    @Test
    public void testGetMainLanguages() {
        // Test case with valid input
        List<Language> languages = new ArrayList<>();
        languages.add(new Language("Java"));
        languages.add(new Language("Python"));

        Comparator<Language> comparator = (l1, l2) -> Integer.compare(l2.getProjectsCount(), l1.getProjectsCount());

        List<Language> result = new LanguageService(new ProjectRepository()).getMainLanguages("org1,org2", comparator, Optional.empty());

        assertEquals(2, result.size());
        assertEquals("Java", result.get(0).getName());
        assertEquals("Python", result.get(1).getName());

        // Test case with empty language list
        List<Language> emptyLanguages = new ArrayList<>();

        List<Language> emptyResult = new LanguageService(new ProjectRepository()).getMainLanguages("org1", comparator, Optional.empty());

        assertTrue(emptyResult.isEmpty());
    }

    @Test
    public void testFilterLanguages() {
        // Test case with valid input
        List<Language> languages = new ArrayList<>();
        languages.add(new Language("Java"));
        languages.add(new Language("Python"));

        List<Language> filtered = new LanguageService(new ProjectRepository()).filterLanguages(languages, 1, 0);

        assertEquals(1, filtered.size());
        assertEquals("Java", filtered.get(0).getName());

        // Test case with limit higher than number of languages
        List<Language> filteredAll = new LanguageService(new ProjectRepository()).filterLanguages(languages, 10, 0);

        assertEquals(2, filteredAll.size());

        // Test case with offset
        List<Language> filteredOffset = new LanguageService(new ProjectRepository()).filterLanguages(languages, 1, 1);

        assertEquals(1, filteredOffset.size());
        assertEquals("Python", filteredOffset.get(0).getName());
    }

}

