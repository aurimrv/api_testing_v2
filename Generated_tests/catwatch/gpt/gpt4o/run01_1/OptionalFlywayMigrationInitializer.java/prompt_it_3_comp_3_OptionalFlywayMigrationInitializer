package org.zalando.catwatch.backend;

import org.flywaydb.core.Flyway;
import org.flywaydb.core.api.configuration.FluentConfiguration;
import org.flywaydb.core.api.migration.FlywayMigrationStrategy;
import org.junit.AfterClass;
import org.junit.Before;
import org.junit.BeforeClass;
import org.junit.Test;
import org.mockito.Mockito;
import org.springframework.jdbc.core.JdbcTemplate;

import static org.junit.Assert.assertNotNull;

public class gpt4o_run01_OptionalFlywayMigrationInitializerTest {

    private static final SutHandler controller = new em.embedded.org.zalando.EmbeddedEvoMasterController();
    private static String baseUrlOfSut;

    @BeforeClass
    public static void initClass() {
        controller.setupForGeneratedTest();
        baseUrlOfSut = controller.startSut();
        controller.registerOrExecuteInitSqlCommandsIfNeeded();
        assertNotNull(baseUrlOfSut);
    }

    @AfterClass
    public static void tearDown() {
        controller.stopSut();
    }

    @Before
    public void initTest() {
        controller.resetDatabase(Arrays.asList("CONTRIBUTOR"));
        controller.resetStateOfSUT();
    }

    @Test
    public void testConstructorExecution() {
        Flyway dummyFlyway = new Flyway(new FluentConfiguration());
        FlywayMigrationStrategy dummyStrategy = flyway -> {};
        OptionalFlywayMigrationInitializer initializer = new OptionalFlywayMigrationInitializer(dummyFlyway, dummyStrategy);
        assertNotNull(initializer);
    }

    @Test
    public void testAfterPropertiesSetDatabaseAvailable() throws Exception {
        Flyway dummyFlyway = new Flyway(new FluentConfiguration());
        FlywayMigrationStrategy dummyStrategy = flyway -> {};
        OptionalFlywayMigrationInitializer initializer = new OptionalFlywayMigrationInitializer(dummyFlyway, dummyStrategy);

        JdbcTemplate mockJdbcTemplate = Mockito.mock(JdbcTemplate.class);
        Mockito.when(mockJdbcTemplate.execute(Mockito.anyString())).thenReturn(null);

        org.zalando.catwatch.backend.repo.util.DatabasePing.setMockedDatabaseAvailability(true);

        initializer.afterPropertiesSet();
    }

    @Test
    public void testAfterPropertiesSetDatabaseUnavailable() throws Exception {
        Flyway dummyFlyway = new Flyway(new FluentConfiguration());
        FlywayMigrationStrategy dummyStrategy = flyway -> {};
        OptionalFlywayMigrationInitializer initializer = new OptionalFlywayMigrationInitializer(dummyFlyway, dummyStrategy);

        JdbcTemplate mockJdbcTemplate = Mockito.mock(JdbcTemplate.class);
        Mockito.when(mockJdbcTemplate.execute(Mockito.anyString())).thenReturn(null);

        org.zalando.catwatch.backend.repo.util.DatabasePing.setMockedDatabaseAvailability(false);

        initializer.afterPropertiesSet();
    }
}

