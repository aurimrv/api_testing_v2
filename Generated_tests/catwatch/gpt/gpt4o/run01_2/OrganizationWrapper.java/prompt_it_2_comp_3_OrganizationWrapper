package org.zalando.catwatch.backend;

import org.junit.AfterClass;
import org.junit.BeforeClass;
import org.junit.Before;
import org.junit.Test;
import static org.junit.Assert.*;
import java.io.IOException;
import java.util.List;
import java.util.Arrays;
import org.kohsuke.github.GHOrganization;
import org.kohsuke.github.GHTeam;
import org.zalando.catwatch.backend.model.OrganizationWrapper;

public class gpt4o_run01_OrganizationWrapperTest {

    private static final SutHandler controller = new em.embedded.org.zalando.EmbeddedEvoMasterController();
    private static String baseUrlOfSut;

    @BeforeClass
    public static void initClass() {
        controller.setupForGeneratedTest();
        baseUrlOfSut = controller.startSut();
        controller.registerOrExecuteInitSqlCommandsIfNeeded();
        assertNotNull(baseUrlOfSut);
        RestAssured.enableLoggingOfRequestAndResponseIfValidationFails();
        RestAssured.useRelaxedHTTPSValidation();
        RestAssured.urlEncodingEnabled = false;
        RestAssured.config = RestAssured.config()
            .jsonConfig(JsonConfig.jsonConfig().numberReturnType(JsonPathConfig.NumberReturnType.DOUBLE))
            .redirect(redirectConfig().followRedirects(false));
    }

    @AfterClass
    public static void tearDown() {
        controller.stopSut();
    }

    @Before
    public void initTest() {
        controller.resetDatabase(Arrays.asList("CONTRIBUTOR"));
        controller.resetStateOfSUT();
    }

    @Test
    public void testConstructorHandlesEmptyRepositories() {
        GHOrganization mockOrg = org.mockito.Mockito.mock(GHOrganization.class);
        org.mockito.Mockito.when(mockOrg.getLogin()).thenReturn("test-org");
        org.mockito.Mockito.when(mockOrg.listRepositories()).thenThrow(new RuntimeException("No repositories found"));

        OrganizationWrapper organizationWrapper = new OrganizationWrapper(mockOrg);

        assertNotNull(organizationWrapper);
        assertTrue(organizationWrapper.listRepositories().isEmpty());
    }

    @Test
    public void testListTeamsHandlesNoTeams() {
        GHOrganization mockOrg = org.mockito.Mockito.mock(GHOrganization.class);
        org.mockito.Mockito.when(mockOrg.getLogin()).thenReturn("test-org");
        org.mockito.Mockito.when(mockOrg.listTeams()).thenThrow(new RuntimeException("No teams found"));

        OrganizationWrapper organizationWrapper = new OrganizationWrapper(mockOrg);
        List<GHTeam> teams = organizationWrapper.listTeams();

        assertNotNull(teams);
        assertTrue(teams.isEmpty());
    }

    @Test
    public void testListTeamsHandlesNonEmptyTeams() throws Exception {
        GHOrganization mockOrg = org.mockito.Mockito.mock(GHOrganization.class);
        org.mockito.Mockito.when(mockOrg.getLogin()).thenReturn("test-org");

        GHTeam mockTeam = org.mockito.Mockito.mock(GHTeam.class);
        org.mockito.Mockito.when(mockOrg.listTeams()).thenReturn(() -> Arrays.asList(mockTeam).iterator());

        OrganizationWrapper organizationWrapper = new OrganizationWrapper(mockOrg);
        List<GHTeam> teams = organizationWrapper.listTeams();

        assertNotNull(teams);
        assertEquals(1, teams.size());
    }

    @Test
    public void testGetPublicRepoCountReturnsZeroOnException() {
        GHOrganization mockOrg = org.mockito.Mockito.mock(GHOrganization.class);
        org.mockito.Mockito.when(mockOrg.getLogin()).thenReturn("test-org");
        org.mockito.Mockito.when(mockOrg.getPublicRepoCount()).thenThrow(new IOException("Public repo count error"));

        OrganizationWrapper organizationWrapper = new OrganizationWrapper(mockOrg);
        int publicRepoCount = organizationWrapper.getPublicRepoCount();

        assertEquals(0, publicRepoCount);
    }

    @Test
    public void testGetPublicRepoCountSuccessful() throws Exception {
        GHOrganization mockOrg = org.mockito.Mockito.mock(GHOrganization.class);
        org.mockito.Mockito.when(mockOrg.getLogin()).thenReturn("test-org");
        org.mockito.Mockito.when(mockOrg.getPublicRepoCount()).thenReturn(42);

        OrganizationWrapper organizationWrapper = new OrganizationWrapper(mockOrg);
        int publicRepoCount = organizationWrapper.getPublicRepoCount();

        assertEquals(42, publicRepoCount);
    }
}

*****
