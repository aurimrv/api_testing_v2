import market.CustomNotValidException;
import market.FieldError;
import market.RestErrorResponse;
import market.WebRequest;
import org.junit.jupiter.api.Test;
import org.springframework.http.HttpStatus;

import static org.junit.jupiter.api.Assertions.assertEquals;
import static org.junit.jupiter.api.Assertions.assertNotNull;

class gpt35_run01_RestExceptionHandlerTest {

    @Test
    void testCustomNotValidException() {
        CustomNotValidException exception = new CustomNotValidException("Test", "Entity", List.of(new FieldError("Object", "field", "defaultMessage")));
        WebRequest request = new WebRequest();
        RestErrorResponse response = controller.customNotValidException(exception, request);

        assertNotNull(response);
        assertEquals(HttpStatus.NOT_ACCEPTABLE.value(), response.getHttpStatus());
        // Add more specific assertions here

        // Cover lines 105, 106, 107, 108 in resolveErrorMessage method
        for (FieldError fieldError : exception.getFieldErrors()) {
            assertNotNull(controller.resolveErrorMessage(fieldError));
        }
    }

    @Test
    void testResolveErrorMessage() {
        FieldError fieldError = new FieldError("Object", "field", "defaultMessage");
        String resolvedError = controller.resolveErrorMessage(fieldError);

        assertNotNull(resolvedError);
        // Add more specific assertions here
    }
}

