import org.junit.jupiter.api.Test;
import org.springframework.validation.FieldError;

import static org.junit.jupiter.api.Assertions.assertThrows;
import static org.junit.jupiter.api.Assertions.assertNotNull;

public class gpt35_run01_RestExceptionHandlerTest {

    @Test
    public void testCustomNotValidException() {
        // Test coverage for method customNotValidException
        CustomNotValidException exception = assertThrows(CustomNotValidException.class, () -> {
            throw new CustomNotValidException("Test Entity", null);
        });
        assertNotNull(exception);
    }

    @Test
    public void testResolveErrorMessage() {
        // Test coverage for method resolveErrorMessage
        RestExceptionHandler restExceptionHandler = new RestExceptionHandler(null);
        FieldError fieldError = new FieldError("ObjectName", "fieldName", "defaultMessage");
        String errorMessage = restExceptionHandler.resolveErrorMessage(fieldError);
        assertNotNull(errorMessage);
    }
}

